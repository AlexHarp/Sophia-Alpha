# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore Drupal's file directory but include config sync
/web/sites/*/files/*
!/web/sites/*/files/config_*/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env
/.editorconfig
/.gitattributes
/gitcreds.txt
/web/sites/default/services.yml


# ignore temp files
*.swp

# ignore working dirs
/web/modules/custom/neptune_sync/resources/
/web/scripts/output
/web/ontology-visualization

# ignore Sophia specific files
/web/neptune_sync.log
/web/neptune_sync_event.log
/web/themes/.gitignore
/web/easyrdf.html

#ignore unused reasources created by composer
/modules
/profiles
/themes
